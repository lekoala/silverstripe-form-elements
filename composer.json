{
    "name": "lekoala/silverstripe-form-elements",
    "description": "A set of form elements for SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "extensions",
        "module",
        "cms",
        "form",
        "elements",
        "custom-element",
        "webcomponent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "LeKoala",
            "email": "thomas@lekoala.be"
        }
    ],
    "require": {
        "php": "^8.1.2",
        "silverstripe/recipe-plugin": "^1 || ^2",
        "silverstripe/recipe-core": "^4.4 || ^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "installer-name": "form-elements",
        "expose": [
            "client"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "LeKoala\\FormElements\\": "src/",
            "LeKoala\\FormElements\\Test\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-form-elements/issues"
    },
    "scripts": {
        "lint": "phpcs src/ tests/",
        "lint-clean": "phpcbf src/ tests/",
        "test": "phpunit -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
